#!/bin/bash
if [ -n "$1" ]; then
  curl -u 'heydemo' https://api.github.com/user/repos -d '{"name":"'"$1"'", "private": "true"}'
  git remote add origin git@github.com:heydemo/${1}.git
  git push -u origin master
else 
  echo "addrepo reponame"
  echo "first create repo / initial commit in current dir"

fi
